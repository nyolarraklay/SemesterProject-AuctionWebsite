{
  "name": "semesterproject-auctionwebsite",
  "version": "0.0.13",
  "description": "auction website",
  "type": "module",
  "main": "/scripts.js",
  "scripts": {
    "test": "npm run test-unit && npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-report": "cypress run --reporter mochawesome",
    "test-e2e-cli": "cypress run",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "build": "sass src/scss:dist/css",
    "watch": "sass --watch src/scss:dist/css & live-server",
    "start": "live-server",
    "dev": "npm-run-all --parallel watch start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyolarraklay/SemesterProject-AuctionWebsite.git"
  },
  "keywords": [
    "css",
    "e-commerce",
    "auction",
    "website"
  ],
  "author": "Ernesto F. Osorio Jr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nyolarraklay/SemesterProject-AuctionWebsite/issues"
  },
  "homepage": "https://github.com/nyolarraklay/SemesterProject-AuctionWebsite#readme",
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@testing-library/jest-dom": "^6.1.5",
    "babel-jest": "^29.7.0",
    "cypress": "^13.6.1",
    "eslint": "^8.53.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "sass": "^1.69.5"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "jest-fetch-mock": "^3.0.3"
  }
}
